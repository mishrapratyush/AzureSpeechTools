{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
    "prettier --single-quote --write",
    "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "1000",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.4",
    "bootstrap": "^5.1.1",
    "dotenv": "^10.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.18.1",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "reactstrap": "^8.10.0",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.2",
    "nodemon": "^2.0.12"
  }
}
